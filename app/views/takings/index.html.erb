<p id="notice"><%= notice %></p>

<h1>Takings</h1>
<%if current_user_type == "professor"then%>
<table>
  <thead>
    <tr>
      <th>Student</th>
      <th>Course</th>
      <th>Group</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @takings.each do |taking| %>
      <tr>
        <td><%= taking.student_id %></td>
        <td><%= taking.course_id %></td>
        <td><%= taking.group_id %></td>
        <!--
        <td><%= link_to 'Show', taking %></td>
        <td><%= link_to 'Edit', edit_taking_path(taking) %></td>
        <td><%= link_to 'Destroy', taking, method: :delete, data: { confirm: 'Are you sure ?' } %></td>
        -->
      </tr>
    <% end %>
  </tbody>
</table>
<%else%>
    <p style="font-size:160%;"> This information is hidden from students and visitors</p>
<%end%>

<br>

